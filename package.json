{
  "private": true,
  "author": "Jerry - li <lishuai4415@gmail.com>",
  "scripts": {
    "dev": "max dev",
    "build": "max build",
    "format": "prettier --cache --write .",
    "prepare": "husky install",
    "postinstall": "max setup",
    "setup": "max setup",
    "start": "npm run dev"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.{js,jsx,ts,tsx,css,less}": [
      "max lint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.4",
    "@ant-design/pro-components": "^2.6.10",
    "@umijs/max": "^4.0.74",
    "ahooks": "^3.7.8",
    "antd": "^5.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "stylelint": "^15.10.2",
    "typescript": "^5.1.6"
  }
}
